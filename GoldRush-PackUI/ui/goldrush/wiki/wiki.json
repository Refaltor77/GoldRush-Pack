{
    "namespace": "gd_wiki",

    "wiki@common_dialogs.main_panel_no_buttons": {
        "$custom_background": "gd_wiki.gd_bg",
        "$show_close_button": true,
        "$use_compact_close_button": false,
        "size": [226,101],
        "controls": [
          {
            "common_panel@common.common_panel": { "$dialog_background": "$custom_background" }
          },
          {
            "label": {
              "type": "label",
              "text": "#form_text",
              "size": ["98%","default"],
              "layer": 1,
              "anchor_from": "center",
              "anchor_to": "center",
              "text_alignment": "center"
            }
          }
        ]
    },

    "gd_bg@common.dialog_background_hollow_common": {
        "type": "image",
        "layer": 1,
        "$fill_alpha": 0.3,
        "texture": "textures/ui/gd_background_wiki",
        "$dialog_background_texture": "textures/ui/gd_background_wiki"
      }
}